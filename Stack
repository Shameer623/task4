package task4;

import java.util.Stack;

// class StackMain
public class StackMain {

//main method
    public static void main(String[] args) {
       Stack<Integer> stack = new Stack<>();
//checking stack is empty?
        boolean empt= stack.empty();
        System.out.println("is stack empty : "+empt);
//adding elements in stack
        stack.push(12);
        stack.push(13);
        stack.push(15);
        stack.push(21);
        System.out.println("elements in the stack : "+stack);
//checking stack is empty?

        empt= stack.empty();
        System.out.println("is stack empty : " +empt);
//removing last element from the stack
        int pop1= stack.pop();
        System.out.println("element removed from the stack : "+pop1);


    }
}

//OUTPUT

is stack empty : true
elements in the stack : [12, 13, 15, 21]
is stack empty : false
element removed from the stack : 21

